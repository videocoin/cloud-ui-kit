{
  "name": "cloud-ui-kit",
  "version": "0.21.1",
  "private": true,
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "sideEffects": false,
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "dependencies": {
    "@types/jest": "24.0.12",
    "@types/node": "12.0.1",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/react-transition-group": "^2.9.1",
    "classnames": "^2.2.6",
